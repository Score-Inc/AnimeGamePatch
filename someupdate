binary=/data/data/com.termux/files/usr/bin/zex
versionBashIn1="2.8"
updateNotif="1. Add NOTE for Root phone in Download Genshin APKs\n2. Add Version download Genshin for 2.7\n3. Add function what difference root and no root\n4. Add Get Certificate\n5. Add code rename back data Genshin (Root)"
updateNote="There is new version to"
link=null
printLatest="This is Latest Version (Ver : $versionBashIn1)"
whatTheFuckEditVersion="Something wrong with version.\nThis version different from the server\n\nServer Latest Version : $versionBashIn1\nVersion Script : $versionBash1\n\nversion too latest from server"
updateScript1() {
    command cd
    updateDir=$HOME/updatezex
    scriptUpdate=$updateDir/zinstall
    if [[ -f $binary ]]; then
       rm $binary
    fi
    mkdir $updateDir
    cd $updateDir
    echo "Update Script!"
    if ! command -v wget &> /dev/null; then
       pkg install wget
    fi
    wget https://raw.githubusercontent.com/ElaXan/AnimeGamePatch/main/zex.sh &> /dev/null
    if [[ $? != 0 ]]; then
       echo "Can't do update Script. Make sure you internet is ON or stable"
       rm $updateDir
       command cd
       exit
    else
       mv zex.sh $binary
       command cd
       rm -rf $updateDir
       command clear
       echo "Update success!"
       sleep 1s
       echo "Settings permission!"
       chmod +x $binary
       sleep 1s
       echo "Done!"
       sleep 1s
       command clear
       echo "Please enter command zex again!"
       cd
       if [[ -f "proxy.py" ]]; then
           rm proxy.py
       fi
       if [[ -f "proxy_config.py" ]]; then
           rm proxy_config.py
       fi
       exit
    fi
}
